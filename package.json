{
  "name": "wm-webapp",
  "version": "1.0.0",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "deploy": "ionic build --prod && scp -r ~/Documents/wm-webapp/www/* server:/var/www/html/app.geohub.webmapp.it/",
    "deploy-webcomponent": "ng build --configuration production --output-hashing=none --output-path=web-component",
    "surge-sardegna": "ionic build --prod && surge --project ./www --domain http://32.webmapp.surge.sh/",
    "surge-sardegna-dev": "ionic build --prod && surge --project ./www --domain http://32.dev-webmapp.surge.sh/",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:run\" --kill-others --success first",
    "e2e-gui": "concurrently \"npm run start\" \"wait-on http-get://localhost:4200 && npm run cy:open\" --kill-others --success first"
  },
  "private": true,
